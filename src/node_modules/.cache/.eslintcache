erval on render\r\n            clearInterval(shirtsInterval); \r\n        }\r\n    });\r\n\r\n    function handleHover(event) {\r\n        // Find the shirt object the user is hovering over, set Remain to shirt object.\r\n        const hoverItem = [...tShirtSales, ...shirtSales].find((item) => {\r\n            return item.name.split(' ').join('') === event.target.id;\r\n        });\r\n        setRemain(hoverItem);\r\n    }\r\n\r\n\r\n    function handleLeave(event) {\r\n    // Set to empty object if the user is hovering over nothing.\r\n    setRemain({});\r\n       \r\n    }\r\n\r\n\r\n    return (<><motion.div\r\n        className='background'\r\n        initial={{ opacity: 0 }}\r\n        animate={{ opacity: 1 }}\r\n        exit={{ opacity: 0 }}\r\n        transition={{ duration: 1 }}\r\n      >\r\n        <h1><i>Our Lastest Deals... </i></h1>\r\n        <h2>Check Out Our T-shirts! </h2>\r\n        <div className=\"mainwindow\">\r\n\r\n        {/*Animate Presence of newly generated shirt displays */}\r\n        <AnimatePresence>\r\n            {mainWindow.map((shirt) => {\r\n                // Wrap each shirt showcased in a Link to the shirt's product description. Covert name of product to tolerable code format.\r\n        return (<Link  className=\"a\" to={\"/product/\" + shirt.name.split(' ').join('+')}>\r\n            {/*Animation settings */}\r\n            <motion.span\r\n        key={shirt.name}\r\n        initial={{ opacity: 0 }}\r\n        animate={{ opacity: 1 }}\r\n        exit={{ opacity: 0 }}\r\n        transition={{ duration: 1.7 }}\r\n      >\r\n        {/* Listener Events added to tags. Name of shirt coverted to tolerable format and used as ID for comparison in handleHover.*/}\r\n        <div id={shirt.name.split(' ').join('')} onMouseLeave={handleLeave}  onMouseEnter={handleHover}  >\r\n        <img alt={shirt.name} id={shirt.name.split(' ').join('')} onMouseEnter={handleHover} src={shirt.image} />\r\n\r\n\r\n      <AnimatePresence>\r\n        {/*Renders price on hover*/}\r\n      <RenderPrice hover={remain} id={shirt} price={shirt.price} name={shirt.name} /> \r\n      </AnimatePresence>\r\n\r\n      </div>\r\n      \r\n      </motion.span> \r\n      </Link>)\r\n    })\r\n    }\r\n\r\n\r\n        </AnimatePresence>\r\n        </div>\r\n        <h2>Best Deals!</h2>\r\n\r\n        <div class=\"categoryContainer\" >\r\n            <h2 style={{textAlign:\"left\", marginLeft:'100px'}}>Shirts on sale 20% off!</h2>\r\n            <div className=\"flexContainer\">\r\n                {/* Formatted for future use implementation of a click to next page of clothing feature.  */}\r\n                {Array(1).fill(0).map((pages) => {\r\n                    {/*Filter for low priced items only */}\r\n                    const page = [shirtSales.filter(div => {\r\n                        return div.price < 30.99;\r\n                    }).map((div,index) => {\r\n                        {/* Do not display more than maxCount */}\r\n                        if (index <= maxCount) {\r\n                            shirtSales.splice(shirtSales.indexOf(div),1);\r\n                            return  <Row div={div} />\r\n                        }\r\n                        return '';\r\n                        }\r\n                        ).filter(element => element !== '')];\r\n                        \r\n                    return <div className=\"row\">{page}</div>;\r\n                })}\r\n            </div>\r\n\r\n        </div>\r\n        <div class=\"categoryContainer\"  >\r\n            <h2 style={{textAlign:\"left\", marginLeft:'100px'}}>Pants on sale 20% off!</h2>\r\n            <div  className=\"flexContainer\">\r\n                {/* Formatted for future use implementation of a click to next page of clothing feature.  */}\r\n                {Array(1).fill(0).map((pages) => {\r\n                     {/*Filter for low priced items only */}\r\n                   const page = [pantsSales.filter(div => {\r\n                   